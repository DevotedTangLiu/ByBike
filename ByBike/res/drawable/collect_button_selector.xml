<?xml version="1.0" encoding="utf-8"?>  
<selector xmlns:android="http://schemas.android.com/apk/res/android">  
    <!-- 获得焦点但未按下时的背景图片 -->
    <item android:drawable="@drawable/collect_button_select"  android:state_pressed="false" android:state_selected="true"/>
    <!-- 按下时的背景图片 -->
    <item android:drawable="@drawable/collect_button_pressed" android:state_enabled="true" android:state_pressed="true"/>
    <!-- 未按下时的背景图片 -->
    <item android:drawable="@drawable/collect_button_nor"  android:state_enabled="true" android:state_pressed="false"/>
    <!-- 默认时的背景图片 -->
    <item android:drawable="@drawable/collect_button_nor"/> 
</selector>  